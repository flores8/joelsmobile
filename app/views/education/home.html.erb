				<%= link_to "learning/example-article" do %>
					<div class="article-small">
						<span class="link"></span>
						<%= link_to "What you should know about the cheap drive through car washes.", "learning/example-article", class: "article-landing-title-small" %>
					</div>
				<% end %>
			</div>	
		</div>


	</div><!-- End Article Wrapper --> 		
</div><!-- End Main Wrapper --> 


